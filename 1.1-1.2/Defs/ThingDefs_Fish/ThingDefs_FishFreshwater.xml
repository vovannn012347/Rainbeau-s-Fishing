<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- TEMPERATE FOREST FISH -->
	
	<RBB.FishDef ParentName="RBB_FishBase" Name="RBB_Catfish">
		<defName>Catfish</defName>
		<label>catfish</label>
		<description>A catfish. Needs to be cleaned before it can be eaten.</description>
		<graphicData>
			<texPath>Catfish</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<statBases>
			<Nutrition>0.75</Nutrition>
			<Mass>1.3</Mass>
			<MarketValue>15</MarketValue>
		</statBases>
		<butcherProducts>
			<RawFishMeat>18</RawFishMeat>
		</butcherProducts>
		<fishProperties>
			<WeightType>Static</WeightType>
			<Weight>12</Weight>
			<TemperatureRange>
				<min>-13</min>
				<fav>10</fav>
				<max>70</max>
			</TemperatureRange>	
			<HumidityRange>
				<min>300</min>
				<fav>1500</fav>
			</HumidityRange>	
			<SwampinessRange>
				<min>0</min>
				<fav>0.6</fav>
				<max>2</max>
			</SwampinessRange>		
			<FreshWaterFactor>3</FreshWaterFactor>	
			<SaltWaterFactor>0</SaltWaterFactor>	
			<RoeDef>Catfish_RoeFish</RoeDef>
			<HasRoe>true</HasRoe>
			<CanBite>true</CanBite>
			<BiteDamageDef>ScratchToxic</BiteDamageDef>
		</fishProperties>
	</RBB.FishDef>
	
	<RBB.FishDef ParentName="RBB_Catfish">
		<defName>Catfish_RoeFish</defName>
		<label>roe catfish</label>
		<statBases>
			<Nutrition>1.725</Nutrition>
			<Mass>1.95</Mass>
			<MarketValue>65</MarketValue>
		</statBases>
		<butcherProducts>
			<FishRoe>13</FishRoe>
		</butcherProducts>
		<fishProperties>
			<WeightType>None</WeightType>
		</fishProperties>
	</RBB.FishDef>
	
	<!-- RAINFOREST FISH -->
	
	<RBB.FishDef ParentName="RBB_FishBase" Name="RBB_Arapaima">
		<defName>Arapaima</defName>
		<label>arapaima</label>
		<description>Arapaima are large freshwater fish native to tropical regions. Needs to be cleaned before it can be eaten.</description>
		<graphicData>
			<texPath>Arapaima</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(2,2)</drawSize>
		</graphicData>
		<statBases>
			<Nutrition>1.25</Nutrition>
			<Mass>3.45</Mass>
			<MarketValue>15</MarketValue>
		</statBases>
		<butcherProducts>
			<RawFishMeat>30</RawFishMeat>
		</butcherProducts>
		<fishProperties>
			<WeightType>Static</WeightType>
			<Weight>9</Weight>
			<TemperatureRange>
				<min>10</min>
				<max>50</max>
			</TemperatureRange>	
			<HumidityRange>
				<min>1800</min>
			</HumidityRange>		
			<SwampinessRange>
				<min>0</min>
				<fav>0</fav>
				<max>0.6</max>
			</SwampinessRange>	
			<HasRoe>true</HasRoe>
			<RoeDef>Arapaima_RoeFish</RoeDef>
			<FreshWaterFactor>1.5</FreshWaterFactor>	
			<SaltWaterFactor>0</SaltWaterFactor>	
			<MarshFactor>0</MarshFactor>
		</fishProperties>
	</RBB.FishDef>

	<RBB.FishDef ParentName="RBB_Arapaima">
		<defName>Arapaima_RoeFish</defName>
		<label>roe arapaima</label>
		<statBases>
			<Nutrition>3.5</Nutrition>
			<Mass>4.95</Mass>
			<MarketValue>135</MarketValue>
		</statBases>
		<butcherProducts>
			<FishRoe>30</FishRoe>
		</butcherProducts>
		<fishProperties>
			<WeightType>None</WeightType>
		</fishProperties>
	</RBB.FishDef>
	
	<RBB.FishDef ParentName="RBB_FishBase" Name="RBB_Piranha">
		<defName>Piranha</defName>
		<label>piranha</label>
		<description>Piranha are tropical fish known for their sharp teeth and powerful jaws. Needs to be cleaned before it can be eaten.</description>
		<graphicData>
			<texPath>Piranha</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<Nutrition>0.375</Nutrition>
			<Mass>0.225</Mass>
			<MarketValue>7.5</MarketValue>
		</statBases>
		<butcherProducts>
			<RawFishMeat>10</RawFishMeat>
		</butcherProducts>
		<fishProperties>
			<WeightType>Static</WeightType>
			<Weight>12</Weight>
			<TemperatureRange>
				<min>5</min>
				<max>55</max>
			</TemperatureRange>	
			<HumidityRange>
				<min>1800</min>
			</HumidityRange>	
			<SwampinessRange>
				<min>0</min>
				<fav>0</fav>
				<max>0.6</max>
			</SwampinessRange>		
			<HasRoe>true</HasRoe>
			<RoeDef>Piranha_RoeFish</RoeDef>
			<FreshWaterFactor>1.5</FreshWaterFactor>	
			<SaltWaterFactor>0</SaltWaterFactor>	
			<MarshFactor>0</MarshFactor>
		</fishProperties>
	</RBB.FishDef>
	
	<RBB.FishDef ParentName="RBB_Piranha">
		<defName>Piranha_RoeFish</defName>
		<label>roe fish</label>
		<statBases>
			<Nutrition>0.825</Nutrition>
			<Mass>0.525</Mass>
			<MarketValue>31.5</MarketValue>
		</statBases>
		<butcherProducts>
			<FishRoe>6</FishRoe>
		</butcherProducts>
		<fishProperties>
			<WeightType>None</WeightType>
		</fishProperties>
	</RBB.FishDef>
</Defs>
