<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- (MOSTLY) OCEAN FISH -->

	<RBB.FishDef ParentName="RBB_FishBase">
		<defName>SeaCucumber</defName>
		<label>sea cucumber</label>
		<description>A sea cucumber. It needs to be butchered.</description>
		<graphicData>
			<texPath>SeaCucumber</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<Nutrition>0.25</Nutrition>
			<Mass>0.2</Mass>
			<MarketValue>10</MarketValue>
		</statBases>
		<butcherProducts>
			<SeaCucumberMeat>7</SeaCucumberMeat>
		</butcherProducts>
		<fishProperties>
			<WeightType>Static</WeightType>
			<Weight>10</Weight>	
			<FreshWaterFactor>0</FreshWaterFactor>	
			<SaltWaterFactor>1</SaltWaterFactor>	
			<MarshFactor>0</MarshFactor>
		</fishProperties>
	</RBB.FishDef>
	
