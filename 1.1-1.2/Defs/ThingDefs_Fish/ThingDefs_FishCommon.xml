			<MarketValue>250</MarketValue>
		</statBases>
		<butcherProducts>
			<Caviar>15</Caviar>
		</butcherProducts>
		<fishProperties>
			<WeightType>None</WeightType>
		</fishProperties>
	</RBB.FishDef>
	

