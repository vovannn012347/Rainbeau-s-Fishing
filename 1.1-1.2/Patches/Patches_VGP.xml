<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
				<xpath>Defs/RecipeDef[defName = "CookMakiVegRoll"]</xpath>
			</li>
			<li Class="PatchOperationInsert">
				<xpath>Defs/RecipeDef[defName = "CookMakiVegRoll"]</xpath>
				<order>Prepend</order>
				<value>
					<RecipeDef>
						<defName>Make_Nigiri</defName>
						<label>Prepare four nigiri</label>
						<description>Prepare nigiri from rice and raw seafood. \n\nMakes 4 rolls. Is a fine meal.</description>
						<jobString>Cooking simple meal.</jobString>
						<workSpeedStat>CookSpeed</workSpeedStat>
						<requiredGiverWorkType>Cooking</requiredGiverWorkType>
						<effectWorking>Cook</effectWorking>
						<soundWorking>Recipe_CookMeal</soundWorking>
						<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
						<ingredients>
							<li>
								<filter>
									<thingDefs>
										<li>RawRice</li>
									</thingDefs>
								</filter>
								<count>1</count>
							</li>
							<li>
								<filter>
									<thingDefs>
										<li>RawFishMeat</li>
										<li>RawSquidMeat</li>
										<li>RawEelMeat</li>
										<li>JellyfishMeat</li>
										<li>PufferfishMeat</li>
										<li>SeaCucumberMeat</li>
										<li>RawShellfishMeat</li>
									</thingDefs>
								</filter>
								<count>1</count>
							</li>
						</ingredients>
						<products>
							<Nigiri>4</Nigiri>
						</products>
						<fixedIngredientFilter>
							<thingDefs>
								<li>RawRice</li>
								<li>RawFishMeat</li>
								<li>RawSquidMeat</li>
								<li>RawEelMeat</li>
								<li>JellyfishMeat</li>
								<li>PufferfishMeat</li>
								<li>SeaCucumberMeat</li>
								<li>RawShellfishMeat</li>
							</thingDefs>
							<specialFiltersToDisallow>
								<li>AllowRotten</li>
							</specialFiltersToDisallow>
						</fixedIngredientFilter>
						<workSkill>Cooking</workSkill>
						<skillRequirements>
							<Cooking>4</Cooking>
						</skillRequirements>
						<recipeUsers>
							<li>ElectricStove</li>
							<li>FueledStove</li>
						</recipeUsers>
					</RecipeDef>
					<RecipeDef>
						<defName>CookMakiRoll</defName>
						<label>prepare four maki roll</label>
						<description>Cooks a somewhat complex meal from a combination of rice rolled with raw fish and/or raw plant ingredients. \n\nMakes 4 rolls. Is a fine meal.</description>
						<jobString>Cooking fine meal.</jobString>
						<workSpeedStat>CookSpeed</workSpeedStat>
						<effectWorking>Cook</effectWorking>
						<soundWorking>Recipe_CookMeal</soundWorking>
						<allowMixingIngredients>true</allowMixingIngredients>
						<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
						<ingredients>
							<li>
								<filter>
									<thingDefs>
										<li>Rawsnowbeet</li>
										<li>Rawbean</li>
										<li>Rawmushroom</li>
										<li>Rawtofu</li>
										<li>PickledVeg</li>
										<li>RawAgave</li>
										<li>RawCoconut</li>
										<li>RawFishMeat</li>
										<li>RawSquidMeat</li>
										<li>RawEelMeat</li>
										<li>JellyfishMeat</li>
										<li>PufferfishMeat</li>
										<li>SeaCucumberMeat</li>
										<li>RawShellfishMeat</li>
									</thingDefs>
								</filter>
								<count>1</count>
							</li>
							<li>
								<filter>
									<thingDefs>
										<li>RawRice</li>
									</thingDefs>
								</filter>
								<count>1</count>
							</li>
						</ingredients>
						<fixedIngredientFilter>
							<thingDefs>
								<li>RawFishMeat</li>
								<li>RawSquidMeat</li>
								<li>RawEelMeat</li>
								<li>JellyfishMeat</li>
								<li>PufferfishMeat</li>
								<li>SeaCucumberMeat</li>
								<li>RawShellfishMeat</li>
								<li>RawRice</li>
								<li>Rawsnowbeet</li>
								<li>Rawbean</li>
								<li>Rawmushroom</li>
								<li>Rawtofu</li>
								<li>PickledVeg</li>
								<li>RawAgave</li>
								<li>RawCoconut</li>
							</thingDefs>
							<specialFiltersToDisallow>
								<li>AllowRotten</li>
							</specialFiltersToDisallow>
						</fixedIngredientFilter>
						<defaultIngredientFilter>
							<thingDefs>
								<li>RawFishMeat</li>
								<li>RawSquidMeat</li>
								<li>RawEelMeat</li>
								<li>JellyfishMeat</li>
								<li>PufferfishMeat</li>
								<li>SeaCucumberMeat</li>
								<li>RawShellfishMeat</li>
								<li>Rawsnowbeet</li>
								<li>Rawbean</li>
								<li>Rawmushroom</li>
								<li>Rawtofu</li>
								<li>PickledVeg</li>
								<li>RawAgave</li>
								<li>RawCoconut</li>
							</thingDefs>
						</defaultIngredientFilter>
						<products>
							<MakiRoll>4</MakiRoll>
						</products>
						<skillRequirements>
							<Cooking>8</Cooking>
						</skillRequirements>
						<recipeUsers>
							<li>ElectricStove</li>
							<li>FueledStove</li>
						</recipeUsers>
					</RecipeDef>
				</value>
			</li>
		</operations>
	</Operation>

</Patch>
