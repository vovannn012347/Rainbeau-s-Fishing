	<RBB.FishDef ParentName="RBB_CrabPotBase">
		<defName>Lobster</defName>
		<label>lobster</label>
		<description>A lobster. Kind of looks like a giant cockroach. Could be eaten raw, but would be much better cooked.</description>
		<graphicData>
			<texPath>Lobster</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<ingestible>
			<preferability>RawBad</preferability>
		</ingestible>
		<statBases>
			<Nutrition>0.6</Nutrition>
			<Mass>0.95</Mass>
			<MarketValue>15</MarketValue>
		</statBases>
		<butcherProducts>
			<RawShellfishMeat>15</RawShellfishMeat>
		</butcherProducts>
		<socialPropernessMatters>true</socialPropernessMatters>
		<fishProperties>
			<WeightType>StaticFactor</WeightType>
			<Weight>0.4</Weight>
			<FreshWaterFactor>0</FreshWaterFactor>	
			<SaltWaterFactor>1</SaltWaterFactor>	
			<MarshFactor>0</MarshFactor>
		</fishProperties>
	</RBB.FishDef>	

